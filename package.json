{
  "name": "messages",
  "description": "Command-line client and tutorial for feathersjs",
  "version": "1.0.0",
  "homepage": "https://github.com/thejones",
  "main": "lib/",
  "bin": {
    "messages": "./bin/messages"
  },
  "keywords": [
    "cli"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/thejones/messages-cli.git"
  },
  "author": {
    "name": "Alex Jones",
    "email": "alexander.t.jones@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/thejones/messages-cli/issues"
  },
  "engines": {
    "node": ">= 5.0.0",
    "npm": ">=3.8.0"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "publish": "git push origin && git push origin --tags",
    "release:patch": "npm version patch && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:major": "npm version major && npm publish",
    "compile": "rm -rf lib/ && babel -d lib/ src/",
    "watch": "babel --watch -d lib/ src/",
    "jshint": "jshint src/. test/. --config",
    "mocha": "mocha test/ --compilers js:babel-core/register",
    "test": "npm run compile && npm run jshint && npm run mocha"
  },
  "directories": {
    "lib": "lib"
  },
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "feathers": "^2.0.1",
    "feathers-authentication": "^0.7.9",
    "feathers-hooks": "^1.5.4",
    "feathers-rest": "^1.4.3",
    "inquirer": "^1.1.0",
    "lodash.merge": "^4.4.0",
    "ora": "^0.2.3",
    "request": "^2.72.0",
    "root-check": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "jshint": "^2.9.1",
    "mocha": "^2.4.5",
    "standard": "^7.1.2"
  }
}
